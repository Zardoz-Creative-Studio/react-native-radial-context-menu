{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ESNEXT",
    "module": "commonjs",
		"jsx": "react-native",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["./node_modules"]
}
